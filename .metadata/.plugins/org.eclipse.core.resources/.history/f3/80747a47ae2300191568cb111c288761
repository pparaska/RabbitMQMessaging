package com.cg.app.rabbitMQDemo.sender;

import org.springframework.stereotype.Component;

@Component
public class Sender {

	// @GeneratedValue(strategy = GenerationType.AUTO)
	private String message;

	public Sender() {
		super();
	}

	public Sender(String message) {
		super();
		this.message = message;
	}

	
}